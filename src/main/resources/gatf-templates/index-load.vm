<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>GATF - Test Reports</title>
		<link href="css/bootstrap.min.css" rel="stylesheet">		
		<style>
			#wrapper {
				width: 100%;
			}

			#page-wrapper {
				padding: 0 15px;
				min-height: 568px;
				background-color: #fff;
			}
			pre {
				background-color: white;
				width;auto;
			}

			handcursor {
				cursor: pointer;cursor: hand;
			}
		</style>

	</head>

	<body>
		<div id="wrapper">		 
			<div id="page-wrapper">
				<!-- /.row -->
				<div class="row">
					<!-- /.col-lg-12 -->
					<div class="col-lg-12">
						<br/>
						<div class="panel panel-default" style="width:500px">
							<div class="panel-heading">
								Final Reports
							</div>
							<!-- /.panel-heading -->
							<div class="panel-body">
								<br/>
								<b><u>Load Test Statistics</u></b>
								<ul style="margin-left:50px" id="stats-pre-ele">
								</ul><br/>
								<b><u>Load Test Sample Runs</u></b>
								<ul style="margin-left:50px" id="stats-pre-ele1">
								</ul>
								<br/>
							</div>
						</div>
					</div>
				</div>
				<!-- /.row -->
			</div>
			<!-- /#page-wrapper -->
		</div>
		<!-- /#wrapper -->

		<!-- Core Scripts - Include with every page -->
		<script src="js/jquery-2.1.0.min.js"></script>
		<script src="js/bootstrap.min.js"></script>
		<script>
			
			var suiteStats = $suiteStats;
			
			var loadTestResources = $loadTestResources;

			$(document).ready(function() {
				$('#stats-pre-ele').html('<li>Total Testcases : '+suiteStats.totalTestCount+'</li> \
					<li>Successful TestCases : '+(suiteStats.totalTestCount - suiteStats.failedTestCount)+'</li> \
					<li>Failed TestCases : '+suiteStats.failedTestCount+'</li> \
					<li>Total ExecutionTime : '+suiteStats.executionTime+'ms</li> \
					<li>Total Multiple Test Runs : '+suiteStats.totalRuns+'</li> \
					<li>Successful Multiple Test Runs : '+(suiteStats.totalRuns - suiteStats.failedRuns)+'</li> \
					<li>Failed Multiple Test Runs : '+suiteStats.failedRuns+'</li>'
				   );
				
				 var html = '';
				 for(var i=0;i<loadTestResources.length;i++) {
					html += '<li><a href="'+loadTestResources[i].url+'">'+loadTestResources[i].title+'</a></li>';
				 }
				 $('#stats-pre-ele1').html(html);

			});
			
		</script>
	</body>
</html>
